{
  "name": "opencodex",
  "private": false,
  "version": "0.2.0",
  "description": "OpenCodex - The open source AI coding agent with a beautiful desktop interface",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "author": "Your Name <your.email@example.com>",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
