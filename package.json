    "lint:check": "eslint src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --cache --cache-location node_modules/.cache/.eslintcache",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
